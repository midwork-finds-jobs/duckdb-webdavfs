# name: test/sql/webdav.test
# description: test webdavfs extension loads correctly
# group: [sql]

# Require statement will ensure this test is run with this extension loaded
require webdavfs

# Test 1: Verify extension is loaded
query I
SELECT COUNT(*) FROM duckdb_extensions() WHERE extension_name = 'webdavfs' AND loaded = true;
----
1

# Test 2: Verify webdavfs configuration settings exist
query I
SELECT COUNT(*) >= 3 FROM duckdb_settings() WHERE name LIKE 'webdav_%';
----
true
